	llx_ticketsup.subject SujetTicket,
llx_ticketsup.ref RefTicket,
	llx_ticketsup.fk_statut StatutTicket,
	llx_commande.ref RefCommande, 
	llx_commande.rowid IDCommande, 
	llx_commande_extrafields.dateenlevementprev EnlevementPrev, 
	llx_commande_extrafields.datelivraisonprev LivraisonPrev, 
	llx_commande_extrafields.cmd_transporteur Transporteur, 
	llx_commande_extrafields.cmd_titre TitreBC, 
	llx_commande_extrafields.refbiotracker RefBiotracker, 
	SuiviTransport(llx_commande_extrafields.demandetpt,llx_commande_extrafields.cmd_transporteur) Tracking, 
	llx_ticketsup.track_id TrackIDTicket, 
	llx_user.lastname RespTicket, 
	IFNULL(DateOperation(llx_commande_extrafields.datelivraisonprev, llx_commande_extrafields.dateenlevementprev, llx_commande_extrafields.date_assemblage, llx_commande.date_livraison, llx_commande_extrafields.cmd_type_tpt,  llx_commande.date_commande),llx_ticketsup_extrafields.deadline) DateOp, 
	WEEK(IFNULL(DateOperation(llx_commande_extrafields.datelivraisonprev, llx_commande_extrafields.dateenlevementprev, llx_commande_extrafields.date_assemblage, llx_commande.date_livraison, llx_commande_extrafields.cmd_type_tpt,  llx_commande.date_commande),llx_ticketsup_extrafields.deadline),3 ) Semaine, 
	llx_projet.ref Projet, 
	llx_projet.rowid IDProjet,
	llx_c_ticketsup_type.label TypeCommande,
    llx_cust_traitement_echantillons_extrafields.nb_colis, 
    llx_cust_traitement_echantillons_extrafields.boites_recues, 
    llx_cust_traitement_echantillons_extrafields.boites_stockees, 
    llx_cust_traitement_echantillons_extrafields.boites_destockees, 
    llx_cust_traitement_echantillons_extrafields.boites_manipulees, 
    llx_cust_traitement_echantillons_extrafields.tubes_controles, 
    llx_cust_traitement_echantillons_extrafields.type_identification, 
    llx_cust_traitement_echantillons_extrafields.niveau_controle, 
    llx_cust_traitement_echantillons_extrafields.notes_echantillons
FROM llx_ticketsup
LEFT JOIN 
	(SELECT llx_commande.rowid bc, llx_ticketsup.ref, llx_ticketsup.track_id track_id
		FROM llx_element_element, llx_ticketsup, llx_commande
			Where llx_element_element.targettype = 'ticketsup' AND llx_element_element.sourcetype = 'commande'
			AND llx_ticketsup.rowid = llx_element_element.fk_target  AND llx_element_element.fk_source = llx_commande.rowid
	UNION
	SELECT llx_commande.rowid bc, llx_ticketsup.ref, llx_ticketsup.track_id track_id
		FROM llx_element_element, llx_ticketsup, llx_commande
			Where llx_element_element.sourcetype = 'ticketsup' AND llx_element_element.targettype = 'commande'
			AND llx_ticketsup.rowid = llx_element_element.fk_source  AND llx_element_element.fk_target = llx_commande.rowid) AS Ticket ON llx_ticketsup.track_id = Ticket.track_id
LEFT JOIN llx_commande ON Ticket.bc = llx_commande.rowid
LEFT JOIN llx_projet ON llx_ticketsup.fk_project = llx_projet.rowid
LEFT JOIN llx_user ON llx_user.rowid = llx_ticketsup.fk_user_assign
LEFT JOIN llx_commande_extrafields ON llx_commande_extrafields.fk_object = llx_commande.rowid
LEFT JOIN llx_ticketsup_extrafields ON llx_ticketsup.rowid = llx_ticketsup_extrafields.fk_object
LEFT JOIN llx_c_ticketsup_type ON llx_c_ticketsup_type.code = llx_ticketsup.type_code
LEFT JOIN llx_cust_traitement_echantillons_extrafields ON llx_cust_traitement_echantillons_extrafields.fk_object = llx_ticketsup.rowid
WHERE llx_ticketsup.category_code IN(&quot;CMD&quot;)
[AND llx_ticketsup.ref = {ticket}]</SQLRaw></SQL><QueryColumns><QueryColumn><Name>SujetTicket</Name><TableName>llx_ticketsup</TableName><ColumnName>subject</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>Opération</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>RefTicket</Name><TableName></TableName><ColumnName></ColumnName><ColumnType></ColumnType><ColumnLength></ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>Ticket</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>StatutTicket</Name><TableName>llx_ticketsup</TableName><ColumnName>fk_statut</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>RefCommande</Name><TableName>llx_commande</TableName>