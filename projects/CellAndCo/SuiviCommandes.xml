AND llx_projet.rowid = llx_commande.fk_projet
AND Ticket.bc = llx_commande.rowid
AND llx_ticketsup.track_id = Ticket.track_id
AND llx_user.rowid = llx_ticketsup.fk_user_assign
AND llx_commande.fk_statut IN(0,1,2)
AND llx_commande.facture = 0
AND TicketWeek.WeekTicket = WEEK(DateOperation(llx_commande_extrafields.datelivraisonprev, llx_commande_extrafields.dateenlevementprev, llx_commande_extrafields.date_assemblage, llx_commande.date_livraison, llx_commande_extrafields.cmd_type_tpt,  llx_commande.date_commande),3)
