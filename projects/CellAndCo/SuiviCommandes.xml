FROM llx_commande, llx_commande_extrafields, llx_ticketsup, llx_user, (SELECT llx_commande.rowid bc, llx_ticketsup.ref, llx_ticketsup.track_id track_id
FROM llx_element_element, llx_ticketsup, llx_commande
Where llx_element_element.targettype = 'ticketsup' AND llx_element_element.sourcetype = 'commande'
AND llx_ticketsup.rowid = llx_element_element.fk_target AND llx_element_element.fk_source = llx_commande.rowid
UNION
SELECT llx_commande.rowid bc, llx_ticketsup.ref, llx_ticketsup.track_id track_id
FROM llx_element_element, llx_ticketsup, llx_commande
Where llx_element_element.sourcetype = 'ticketsup' AND llx_element_element.targettype = 'commande'
AND llx_ticketsup.rowid = llx_element_element.fk_source AND llx_element_element.fk_target = llx_commande.rowid) AS Ticket, llx_projet,(SELECT WEEK(llx_ticketsup_extrafields.deadline,3) WeekTicket, COUNT(llx_ticketsup.ref) NbTicket FROM llx_ticketsup, llx_ticketsup_extrafields WHERE llx_ticketsup.rowid = llx_ticketsup_extrafields.fk_object AND YEAR(llx_ticketsup_extrafields.deadline) = YEAR(NOW()) GROUP BY WEEK(llx_ticketsup_extrafields.deadline,3)) AS TicketWeek
